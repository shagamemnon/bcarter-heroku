<div id="panel" class="panel slideout-panel slideout-panel-right">
    <header class="panel-header">
      <button class="btn-hamburger js-slideout-toggle"><span>â˜°</span><a class="header-font">Menu</a></button>
  </header>

</div>
<header id="default-nav-header" class="nav-header nav-header-fixed animated">
    <a id="home-button" href="{{@blog.url}}/#open">
{{!--         <img src="{{asset "img/icons/icon.png"}}" alt="{{@blog.title}} icon" class="icon rounded hvr-buzz-out" /> --}}
        <img src="{{@blog.logo}}" alt="{{@blog.title}} avatar" class="hvr-buzz-out" />
