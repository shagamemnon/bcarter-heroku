        </li>
        {{/foreach}}
        <li class="subscribe-button nav-blog hide {{#if current}} nav-current{{/if}}">
            <a class="subscribe-link" href="{{@blog.url}}/#open" title="subscribe">Subscribe</a>
        </li>
      </ul>
    </section>
</nav>
