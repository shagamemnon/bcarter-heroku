<nav id="menu" class="menu slideout-menu slideout-menu-right">
  <h3>BrandonCarter.com</h3>
    <section class="menu-section">
      <ul class="menu-section-list">
        {{#foreach navigation}}
        <li class="nav-{{slug}} {{#if current}} nav-current{{/if}}">
